<html>
<head>
	
<title>Go to File</title>
<link rel="stylesheet" type="text/css" href="file://<%= asset_path_for_html %>/gotofile.css">

<script type="text/javascript">
//<![CDATA[
   <%= js_vars %>
//]]>
</script>
<script src="file://<%= asset_path_for_html %>/gotofile.js" type="text/javascript"></script>
	
</head>
<body>
	<div id='head'>
		<input accesskey="f" tabindex="0" id="search" type='search' incremental autosave="TMFFFSearch" results="20" size="25" placeholder="look for files" onsearch="GoToFile.handle_search(this.value);">
	</div>
	<div id="progress">
		<img class="progress_image" src="file://<%= asset_path_for_html %>/progress_wheel.gif">
	</div>
	<div id="debug"></div>
	<div id="result"></div>
	<div id="footer"><div id="footer_progress"><div id="footer_progress_text"></div></div></div>
</body>
</html>